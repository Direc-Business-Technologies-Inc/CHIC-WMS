		function NoNegativeNumber(event) {
			// Get the input key code
			var keyCode = event.keyCode || event.which;

			// Test if the input matches the pattern
			if (keyCode === 189) {
				
				// If the input contains disallowed characters, prevent the default behavior (typing)
				event.preventDefault();
			}
		}
